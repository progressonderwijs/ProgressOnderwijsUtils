<Project Sdk="Microsoft.NET.Sdk">
    <Import Project="..\NugetPackagesCommon.props" />
    <PropertyGroup Label="Configuration">
        <Version>1.0.0</Version>
        <PackageReleaseNotes>Separate the WebDriverPool into its own package.</PackageReleaseNotes>

        <Title>ProgressOnderwijsUtils.SeleniumWebDriverPool</Title>
        <Description>Provides a pooling mechanism for webdrivers to avoid paying the high startup cost for each instance repeatedly</Description>
        <PackageTags>ProgressOnderwijsUtils Selenium WebDriver Web</PackageTags>
        <TargetFramework>net6.0</TargetFramework>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Selenium.WebDriver" Version="4.3.0" />
    </ItemGroup>
</Project>


