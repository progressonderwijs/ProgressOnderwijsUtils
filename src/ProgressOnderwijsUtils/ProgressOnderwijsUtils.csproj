<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup Label="Configuration">
    <Version>14.2.0</Version>
    <Authors>Progress Onderwijs</Authors>
    <Owners>Progress Onderwijs</Owners>
    <Title>ProgressOnderwijsUtils</Title>
    <Description>
      Various utility methods+classes used by Progress Onderwijs.
    </Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>Add RsaStringSigner</PackageReleaseNotes>
    <PackageTags>ProgressOnderwijs</PackageTags>
    <PackageLicenseUrl>http://www.apache.org/licenses/LICENSE-2.0</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/progressonderwijs/ProgressOnderwijsUtils</PackageProjectUrl>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFrameworks>net461;netstandard2.0</TargetFrameworks>
  </PropertyGroup>
  <PropertyGroup>
    <OutputType>library</OutputType>
    <DefineConstants>TRACE;JETBRAINS_ANNOTATIONS</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Include="SingleSignOn\*.xsd" />
    <EmbeddedResource Include="SingleSignOn\*.xsd.intellisensehack" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" Version="11.1.0" />
    <PackageReference Include="ExpressionToCodeLib" version="2.5.1" />
    <PackageReference Include="log4net" version="2.0.8" />
    <PackageReference Include="morelinq" version="2.8.0" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.4.0" />
    <PackageReference Include="System.Data.SqlClient" Version="4.4.0" />
    <PackageReference Include="System.Security.Cryptography.Xml" Version="4.4.0" />
    <PackageReference Include="ValueUtils" version="1.4.0" />
    <PackageReference Include="DotNetZip" Version="1.10.1" NoWarn="NU1701" />
    <PackageReference Include="ZlibWithDictionary" version="2.1.1" NoWarn="NU1701" />
    <PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="2.0.0" />
  </ItemGroup>
</Project>
