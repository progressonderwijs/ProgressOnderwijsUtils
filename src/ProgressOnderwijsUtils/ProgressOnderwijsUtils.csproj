<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup Label="Configuration">
    <Version>8.0.0</Version>
    <Authors>Progress Onderwijs</Authors>
    <Owners>Progress Onderwijs</Owners>
    <Title>ProgressOnderwijsUtils</Title>
    <Description>
      Various utility methods+classes used by Progress Onderwijs.
    </Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>BulkCopyToSqlServer no longer specifies UseInternalTransaction</PackageReleaseNotes>
    <PackageTags>ProgressOnderwijs</PackageTags>
    <PackageLicenseUrl>http://www.apache.org/licenses/LICENSE-2.0</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/progressonderwijs/ProgressOnderwijsUtils</PackageProjectUrl>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFrameworks>net46</TargetFrameworks>
  </PropertyGroup>
  <PropertyGroup>
    <OutputType>library</OutputType>
    <DefineConstants>TRACE;JETBRAINS_ANNOTATIONS</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Include="SingleSignOn\*.xsd" />
    <EmbeddedResource Include="SingleSignOn\*.xsd.intellisensehack" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.ComponentModel.DataAnnotations" />
    <Reference Include="System.Security" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Web" />

    <PackageReference Include="JetBrains.Annotations" Version="11.0.0" />
    <PackageReference Include="EcmaScript.Net" version="1.0.1.0" />
    <PackageReference Include="ExpressionToCodeLib" version="2.5.0" />
    <PackageReference Include="log4net" version="2.0.8" />
    <PackageReference Include="morelinq" version="2.7.0" />
    <PackageReference Include="ValueUtils" version="1.4.0" />
    <PackageReference Include="System.ValueTuple" version="4.4.0" />
    <PackageReference Include="ZlibWithDictionary" version="2.1.1" />
  </ItemGroup>
</Project>
